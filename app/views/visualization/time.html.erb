   
<h1> Time Taken </h1>

<h3>Parallel Processing </h3>
<table class="table table-striped table-bordered table-condensed">
  <tr>
    <td> File1:
    </td>
    <td> File2:
    </td>
    <td> Similarity:
    </td>
    <td> Time:
    </td>
  </tr>
<% @time_taken_parallel.each do |t| %>
  <tr>
    <td>
        <%=t.file1%>
    </td>
    <td>
      <%=t.file2 %>
    </td>
    <td>
        <%=t.similarity%>
    </td>
    <td>
      <%=t.time %>
    </td>
   </tr>

<% end %>
</table>


